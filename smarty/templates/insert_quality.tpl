<div class="input-group mb-3">
  <span class="input-group-text" id="qualityName">Name:</span>
  <input type="text" class="form-control" name="qualityName" aria-label="Sizing example input" aria-describedby="qualityName" required>
</div>
<div class="input-group mb-3">
	<span class="input-group-text" id="lowTemperature">Low Temperature:</span>
	<input class="form-control" type="number" name="lowTemperature" min="0" max="999999" required> 
</div>
<div class="input-group mb-3">
	<span class="input-group-text" id="highTemperature">High Temperature:</span>
	<input class="form-control" type="number" name="highTemperature" min="0" max="999999" required> 
</div>